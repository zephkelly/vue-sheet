{
    "name": "@zephkelly/vue-sheet-lib",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "license": "MIT",
    "description": "Performant sheet components for Vue-based applications",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/zephkelly/vue-sheet.git"
    },
    "bugs": {
        "url": "https://github.com/zephkelly/vue-sheet/issues"
    },

    
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
        "import": "./dist/index.js",
        "types": "./dist/index.d.ts"
        },
        "./style.css": "./dist/style.css"
    },
    "files": [
        "dist"
    ],


    "scripts": {
        "build": "vite build && vue-tsc --emitDeclarationOnly",
        "dev": "vite build --watch",
        "clean": "rm -rf dist"
    },


    "peerDependencies": {
        "vue": "^3.5.13"
    },
    "devDependencies": {
        "@vitejs/plugin-vue": "^5.2.3",
        "vue": "^3.5.13",
        "vue-tsc": "^2.2.8"
    }
}